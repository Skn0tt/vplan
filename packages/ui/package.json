{
  "name": "vplan-ui",
  "version": "0.2.2",
  "description": "the ui for vplan",
  "main": "src/index.html",
  "repository": "https://gitlab.com/skn0tt/vplan",
  "author": "Simon Knott",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn parcel src/index.html -o index.html",
    "watch": "yarn parcel watch src/index.html --public-url / -o index",
    "build":
      "yarn parcel build src/index.html --public-url / -o index && yarn serviceworker",
    "serviceworker": "yarn workbox generateSW",
    "docker:replace": "docker exec vplan_ui_1 /usr/local/bin/replace"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "create-react-class": "^15.6.3",
    "es6-promise": "^4.2.4",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.39",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-tappable": "^1.0.2",
    "rxjs": "^5.5.8",
    "vplan-redux": "^0.2.2",
    "vplan-util": "^0.2.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.1.0",
    "@types/lodash": "^4.14.104",
    "@types/react-dom": "^16.0.5",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.6.2",
    "tslib": "^1.9.0",
    "typescript": "^2.7.2",
    "vplan-types": "^0.2.0",
    "workbox-build": "^3.0.1",
    "workbox-cli": "^3.0.1"
  },
  "swCacheId": "vplan-ui"
}
